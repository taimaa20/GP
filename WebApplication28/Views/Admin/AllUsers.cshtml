@model IEnumerable<WebApplication28.Models.User>

@{

    Layout = "~/Views/Shared/_AdminLayout.cshtml";
    var countOFEmployees= ViewBag.countOFEmployees ;

    var countOFUsers = ViewBag.countOFUsers; 
       string[] TableHeaders = new string[]{"UserID","UserImage" ,"Fname"
          ,"Lname"," Gender"," Email","PhoneNumber","Adress","City","Birthdate"
     };

}


<div class="table">
    <table class="table table-bordered table-hover" id="dataTable">
        <thead>
            <tr>
                @{
                    foreach (var head in TableHeaders)
                    {
                        <th>
                            @head
                        </th>
                    }
                }
            </tr>
        </thead>
        <tbody>

            @{
                
                 @if (Model != null)
            {
                foreach (var data in Model)
             
                {

                    <tr>
                        <td>@data.UserId </td>
                        <td>@data.UserImage </td>
                        <td>@data.FirstName </td>

                        <td>@data.LastName </td>
                        <td>@data.Gender </td>
                        <td>@data.Email</td>
                        <td>@data.PhoneNumber</td>
                         <td>@data.Adress</td>
                          <td>@data.City</td>
                           <td>@data.BirthDate</td>

                       
                    </tr>


                }
            }
            }
        </tbody>
    </table>
   
</div>
